{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "types": [
      "node"
    ],
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "lib": [
      "es2015",
      "dom"
    ]
  }
}